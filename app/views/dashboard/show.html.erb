<div class="container-fluid show-page">
  <div class="col-md-6-offset-2">
    <h2>Welcome <%= current_user.name %></h2>
      <div class="col-md-6-offset-2 search-field">
        <%= form_tag("/search", method: "get") do %>
          <%= label_tag(:q, "Search for:") %>
          <%= text_field_tag(:q) %>
          <%= submit_tag("Search") %>
        <% end %>
    </div>
  </div>
</div>
